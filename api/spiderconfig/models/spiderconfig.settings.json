{
  "connection": "default",
  "collectionName": "spiderconfigs",
  "info": {
    "name": "Spiderconfig"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "spider_name": {
      "type": "string"
    },
    "settings": {
      "type": "json"
    },
    "crawl_kwargs": {
      "type": "json"
    },
    "use_fargate": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "other_kwargs": {
      "type": "json"
    },
    "rate": {
      "type": "string"
    },
    "cron": {
      "type": "string"
    }
  }
}
